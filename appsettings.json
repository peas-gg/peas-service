{
    "AllowedHosts": "*",
    "ApplicationInsights": {
        "InstrumentationKey": "#{AppInsigts_InstrumentationKey}#"
    },
    "Environment": "#{Environment}#",
    "ConnectionStrings": {
        "Database": "#{Database}#"
    },
    "Twilio": {
        "AccountSid": "#{Twilio_AccountSid}#",
        "AuthToken": "#{Twilio_AuthToken}#",
        "PathServiceSid": "#{Twilio_PathServiceSid}#"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    }
}